{
  "name": "osmnearbynotesviewer",
  "version": "0.0.1",
  "description": "Reply to OSM Notes like comment form around home location.",
  "scripts": {
    "postinstall": "node -e \"require('shelljs').exec('typings install');\"",
    "prebuild": "npm run clean",
    "build": "node -e \"require('shelljs').exec('npm-run-all ts:build-* test jade mkdir-builddir browserify');\"",
    "postbuild": "node -e \"require('shelljs').exec('npm-run-all copy-* clean');\"",
    "clean": "node -e \"require('shelljs').rm('-R', ['temp', 'build']);\"",
    "copy-src": "node -e \"require('shelljs').cp('-R', 'src/css', 'build')\"",
    "jade": "node -e \"require('shelljs').exec('jade src/views -P -o build');\"",
    "mkdir-builddir": "node -e \"require('shelljs').mkdir('build/js');\"",
    "browserify": "node -e \"require('shelljs').exec('browserify --entry ./temp/browser/main.js -o build/js/main.js');\"",
    "ts:build-browser": "tsc -p ./src/ts/browser",
    "test": "node -e \"require('shelljs').exec('mocha --require ./src/test/espower-typescript-loader src/test/**.ts');\""
  },
  "keywords": [
    "OpenStreetMap",
    "OSM"
  ],
  "author": "tom-konda",
  "license": "MIT",
  "dependencies": {
    "es6-promise": "^3.1.2",
    "leaflet.icon.glyph": "^0.2.0",
    "osm-auth": "^0.2.8"
  },
  "devDependencies": {
    "browserify": "^12.0.1",
    "espower-typescript": "^4.1.0",
    "jade": "^1.11.0",
    "mocha": "^2.4.5",
    "node-oauth": "^0.1.3",
    "node-sass": "^3.4.2",
    "npm-run-all": "^1.5.1",
    "power-assert": "^1.2.0",
    "shelljs": "^0.6.0",
    "should": "^8.2.2",
    "tslint": "^3.5.0",
    "tslint-eslint-rules": "^1.0.1",
    "typedoc": "^0.3.12",
    "xml2js": "^0.4.16"
  },
  "engines": {
    "node": ">=4.3.0"
  }
}
